nc: faSync,\n  tag: faTag,\n  target: faBullseye,\n  upload: faUpload,\n  users: faUsers\n}\n\nexport const UiButton = ({ icon, children, ...rest }) => {\n  const theIcon = icon && icons[icon]\n  return (\n    <Button {...rest}>\n      {icon && (\n        <Icon>\n          <FontAwesomeIcon icon={theIcon} />\n        </Icon>\n      )}\n      {children && <span>{children}</span>}\n    </Button>\n  )\n}\n\nUiButton.propTypes = {\n  icon: PropTypes.oneOf(Object.keys(icons)),\n  children: PropTypes.any\n}\n"]}]}
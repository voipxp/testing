 faTimes,\n  faTrash,\n  faUpload,\n  faUserPlus,\n  faUsers,\n  faWrench\n} from '@fortawesome/free-solid-svg-icons'\n\nconst icons = {\n  add: faPlus,\n  bulk: faSitemap,\n  cancel: faTimes,\n  check: faCheck,\n  clone: faClone,\n  cogs: faCogs,\n  delete: faTrash,\n  device: faWrench,\n  download: faDownload,\n  edit: faCog,\n  info: faInfo,\n  left: faChevronLeft,\n  link: faLink,\n  list: faList,\n  lock: faLock,\n  open: faExternalLinkAlt,\n  right: faChevronRight,\n  search: faSearch,\n  select: faUserPlus,\n  sync: faSync,\n  tag: faTag,\n  target: faBullseye,\n  upload: faUpload,\n  users: faUsers\n}\n\nexport const UiButton = ({ icon, children, ...rest }) => {\n  const theIcon = icon && icons[icon]\n  return (\n    <Button {...rest}>\n      {icon && (\n        <Icon>\n          <FontAwesomeIcon icon={theIcon} />\n        </Icon>\n      )}\n      {children && <span>{children}</span>}\n    </Button>\n  )\n}\n\nUiButton.propTypes = {\n  icon: PropTypes.oneOf(Object.keys(icons)),\n  children: PropTypes.any\n}\n"]}]}
{
  "name": "odinweb",
  "version": "3.17.16",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production gulp",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\" -l",
    "format": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\" --write",
    "fix": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "dev": "live-server --proxy=/api:http://127.0.0.1:${API_PORT:-80}/api --proxy=/socket.io:http://127.0.0.1:${SOCKET_PORT:-4000}/socket.io --proxy=/audio:http://127.0.0.1:${AUDIO_PORT:-5000} dist/",
    "watch": "gulp watch",
    "browsers": "npx browserslist"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "angular": "^1.7.2",
    "angular-animate": "^1.7.2",
    "angular-cache": "^4.6.0",
    "angular-chart.js": "^1.1.1",
    "angular-color-picker": "^1.0.11",
    "angular-jwt": "^0.1.8",
    "angular-marked": "^1.2.2",
    "angular-papaparse": "^1.0.0",
    "angular-route": "^1.7.2",
    "angular-sanitize": "^1.7.2",
    "angular-truncate-2": "^0.4.2",
    "animate.css": "^3.5.2",
    "buffer-to-vinyl": "^1.1.0",
    "bulma": "^0.7.1",
    "bulma-extensions": "^2.2.1",
    "checklist-model": "^1.0.0",
    "clipboard": "^2.0.0",
    "gulp-ng-config": "^1.5.0",
    "localforage": "^1.5.5",
    "lodash": "^4.17.2",
    "moment": "^2.20.1",
    "mustache": "^3.0.1",
    "ng-idle": "^1.3.2",
    "npm": "^6.0.0",
    "papaparse": "^4.3.6",
    "rrule": "2.2.0",
    "socket.io-client": "^2.2.0",
    "sugar-date": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@fortawesome/fontawesome-free": "^5.4.1",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "babel-preset-minify": "^0.5.0",
    "del": "^3.0.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.3.0",
    "gulp": "^4.0.0",
    "gulp-angular-templatecache": "^2.2.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "lint-staged": "^8.1.0",
    "live-server": "^1.2.0",
    "prettier": "^1.13.7",
    "run-sequence": "^2.2.1",
    "stream-series": "^0.1.1",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  }
}

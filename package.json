{
  "name": "angular",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html --detailed-report",
    "prettier": "prettier \"src/**/*\" -l",
    "format": "yarn prettier --write",
    "fix": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "browsers": "npx browserslist"
  },
  "assetsPath": "assets",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "angular": ">=1.x <2",
    "angular-animate": "^1.7.7",
    "angular-cache": "^4.6.0",
    "angular-chart.js": "^1.1.1",
    "angular-color-picker": "^1.0.11",
    "angular-jwt": "^0.1.10",
    "angular-marked": "^1.2.2",
    "angular-papaparse": "^1.0.0",
    "angular-route": "^1.7.7",
    "angular-sanitize": "^1.7.7",
    "angular-truncate-2": "^0.4.2",
    "animate.css": "^3.7.0",
    "bulma": "^0.7.4",
    "bulma-extensions": "^6.2.1",
    "localforage": "^1.7.3",
    "marked": "^0.6.1",
    "ng-idle": "^1.3.2",
    "papaparse": "^4.6.3",
    "socket.io": "^2.2.0",
    "sugar-date": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "parcel": "^1.11.0",
    "parcel-plugin-asset-copier": "^1.0.0",
    "parcel-plugin-clean-dist": "^0.0.6",
    "sass": "^1.17.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ],
    "src/**/*": [
      "prettier --write",
      "git add"
    ]
  }
}
